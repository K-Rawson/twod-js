<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>2d-JS HTML5 Rendering Engine</display-name>

	<listener>
		<listener-class>com.google.js.twod.WorldInitializer</listener-class>
	</listener>

	<servlet>
		<servlet-name>WorldService</servlet-name>
		<servlet-class>com.google.js.twod.WorldService</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>WorldService</servlet-name>
		<url-pattern>/services/world</url-pattern>
	</servlet-mapping>
</web-app>